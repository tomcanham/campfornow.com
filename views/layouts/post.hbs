{{!< page}}
<header class="blog-header">
  <div class="blog-title">
    {{{block "headerText"}}}
  </div>
</header>
<div class="blog-body container">
  <div class="row row-offcanvas row-offcanvas-right">
    <div class="blog-content col-xs-12 col-sm-9">
      {{{body}}}
    </div>
    <div class="blog-right col-xs-6 col-sm-3" id="sidebar">
      {{> blogRight}}
    </div>
  </div>
</div>